{
	"name": "mixpanel-dnd",
	"description": "drag, drop, and map files into mixpanel (ETL style)",
	"private": false,
	"version": "0.0.1",
	"type": "module",
	"author": {
		"name": "AK",
		"email": "ak@mixpanel.com"
	},
	"bin": {
		"mixpanel-dnd": "./cli.js"
	},
	"scripts": {
		"dev": "vite",
